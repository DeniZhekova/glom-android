<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid
                android:color="#FFAA00" />
            <stroke
                android:width="2dip"
                android:color="#FFF" />
            <padding
                android:left="5dip"
                android:right="5dip"
                android:top="5dip"
                android:bottom="5dip" />
            <size
                android:height="30dip"
                android:width="30dip"
                />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_action_place"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"/>
</layer-list>